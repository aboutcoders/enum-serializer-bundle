{
    "name": "aboutcoders/enum-serializer-bundle",
    "type": "symfony-bundle",
    "description": "A symfony bundle to serialize/deserialize enumerations of type myclabs/php-enum",
    "keywords": ["enum", "myclabs/php-enum", "serializer", "jms"],
    "homepage": "https://bitbucket.org/aboutcoders/enum-serializer-bundle",
    "license": "AboutCoders",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "hschulz@aboutcoders.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.1",
        "myclabs/php-enum": "~1.4",
        "jms/serializer-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": { "Abc\\Bundle\\EnumSerializerBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}